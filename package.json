{
  "name": "@wmfs/tymly-statelint",
  "version": "0.0.0-sematically-released",
  "description": "Validator for Amazon States Language with Tymly Extensions JSON.",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/tymly-statelint#readme",
  "keywords": [
    "tymly",
    "package",
    "state machines",
    "aws"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/tymly-statelint.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/tymly-statelint/issues"
  },
  "main": "lib/index.js",
  "bin": {
    "tymly-statelint": "bin/tymly-statelint.js"
  },
  "dependencies": {
    "@wmfs/j2119": "1.7.1",
    "@wmfs/statelint": "1.7.1",
    "ajv": "6.5.5",
    "lodash.clonedeep": "4.5.0",
    "lodash.kebabcase": "4.1.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/git": "7.0.15",
    "chai": "4.2.0",
    "codecov": "3.5.0",
    "conventional-changelog-metahub": "2.0.2",
    "cz-conventional-changelog": "2.1.0",
    "dirty-chai": "2.0.1",
    "mocha": "6.1.4",
    "nyc": "13.1.0",
    "semantic-release": "15.13.17",
    "standard": "12.0.1"
  },
  "scripts": {
    "tymly-statelint": "./bin/tymly-statelint.js",
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
